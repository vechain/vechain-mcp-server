{
  "mcpServers": {
    "vechain": {
      "command": "npx",
      "args": [
        "-y",
        "@vechain/mcp-server@latest"
      ],
      "env": {
        "VECHAIN_NETWORK": "mainnet"
      }
    },
    "vechain-local": {
      "command": "node",
      "args": [
        "${workspaceFolder}/dist/stdio.js"
      ]
    },
    "vechain-local-dev": {
      "url": "http://localhost:4000/mcp"
    }
  }
}
